# Create a new blueprint
# id : The id to match to. Required
# name : The name
# pattern : The available slots to open in the constructor. 1 for stick, 2 is for material, 0 for nothing
# CustomModelData : The custom model data to apply to "minecraft:paper" for the blueprint item
data modify storage tinkery:core blueprints.new set value {name:"Unknown Blueprint",pattern:[0,0,0,0,0,0,0,0,0],CustomModelData:4041000}